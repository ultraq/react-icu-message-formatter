{
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:import/warnings",
		"plugin:react/recommended",
		"ultraq"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
