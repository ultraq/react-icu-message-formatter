{
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"plugin:jsdoc/recommended",
		"plugin:react/recommended",
		"ultraq"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020
	},
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"rules": {
		"jsdoc/no-undefined-types": ["error", {
			"definedTypes": ["JSX"]
		}]
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
