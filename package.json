{
	"name": "@ultraq/react-icu-message-formatter",
	"version": "0.10.1",
	"description": "React wrapper for the lightweight ICU message formatter",
	"author": "Emanuel Rabina <emanuelrabina@gmail.com> (http://www.ultraq.net.nz/)",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ultraq/react-icu-message-formatter.git"
	},
	"keywords": [
		"react",
		"icu",
		"message",
		"format",
		"intl",
		"i18n"
	],
	"module": "lib/react-icu-message-formatter.es.js",
	"main": "lib/react-icu-message-formatter.cjs.js",
	"types": "index.d.ts",
	"sideEffects": false,
	"scripts": {
		"lint": "eslint \"**/*.js\"",
		"test": "jest",
		"build": "npm run build:lib && npm run build:dts",
