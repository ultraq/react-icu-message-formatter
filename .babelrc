{
	"presets": [
		["@babel/preset-env", {
			"targets": {
				"browsers": [
					"ie 11"
				]
			}
		}],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-runtime",
		["transform-react-remove-prop-types", {
			"mode": "wrap"
		}]
	],
	"env": {
		"test": {
			"presets": [
				["@babel/preset-env", {
					"targets": {
						"node": "current"
					}
				}]
			]
		}
	}
}
